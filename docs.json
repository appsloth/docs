{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Appsloth",
  "colors": {
    "primary": "#f2994a",
    "light": "#f2994a",
    "dark": "#f2994a"
  },
  "background": {
    "color": {
      "light": "#FFFBF5"
    }
  },
  "favicon": "/logo/light.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["introduction", "quickstart"]
          },
          {
            "group": "Features",
            "pages": [
              "features/banners",
              "features/feedback",
              "features/inbox",
              "features/retention-modal",
              "features/status"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://appsloth.com/community",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://appsloth.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://appsloth.com/app"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
